{
  "name": "swapperd-desktop",
  "version": "1.0.0",
  "description": "SwapperD Desktop menubar application for MacOS, Windows and Linux",
  "main": "dist/main/main.js",
  "scripts": {
    "lint:unused-exports": "ts-unused-exports ./tsconfig.json",
    "lint:outdated-imports": "ncu",
    "lint:ts": "tslint --project ./ -t stylish --quiet",
    "lint:extra": "npm run --silent lint:unused-exports; npm run --silent lint:outdated-imports",
    "lint:all": "npm run --silent lint:ts && npm run --silent lint:extra",
    "lint": "npm run --silent lint:ts",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('This repository has been set up with Yarn, not NPM.')\"",
    "build:main": "tsc",
    "postinstall": "electron-builder install-app-deps",
    "start": "NODE_ENV=development electron .",
    "dev": "yarn run build:main; concurrently -k 'sleep 1; yarn run start' 'node -r @babel/register src/dev/server.js'",
    "build": "yarn run build:main; NODE_ENV=production webpack --config ./config/webpack.config.production.babel.js",
    "package": "yarn run build && electron-builder"
  },
  "author": "Ren",
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "build": {
    "appId": "com.github.republicprotocol.swapperd-native",
    "mac": {
      "target": [
        "zip",
        "dmg"
      ],
      "category": "public.app-category.developer-tools",
      "icon": "resources/app.icns"
    },
    "productName": "SwapperD Native",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "resources/",
      "index.html",
      "package.json"
    ],
    "publish": [{
      "provider": "github",
      "owner": "republicprotocol",
      "repo": "swapperd-native-releases"
    }]
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/node-notifier": "^0.0.28",
    "@types/react-redux": "^7.0.0",
    "@types/sqlite3": "^3.1.3",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "bcrypt": "^3.0.3",
    "bignumber.js": "^8.0.2",
    "body-parser": "^1.18.3",
    "csstype": "^2.6.2",
    "electron-updater": "^4.0.6",
    "express": "^4.16.4",
    "extract-zip": "^1.6.7",
    "fix-path": "^2.1.0",
    "immutable": "^4.0.0-rc.12",
    "menubar": "^5.2.3",
    "moment": "^2.24.0",
    "node-notifier": "^5.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-hot-loader": "4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "2.88.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.0.6",
    "typesafe-actions": "^3.0.0",
    "url-loader": "^1.1.2",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.20",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "aws-sdk": "^2.395.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.3",
    "electron-builder": "^20.38.5",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "node-pre-gyp": "^0.12.0",
    "node-sass": "^4.11.0",
    "npm-check-updates": "^2.15.0",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "ts-unused-exports": "^2.0.11",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  }
}