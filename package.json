{
    "name": "swapperd-native",
    "homepage": ".",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.18.0",
        "bignumber.js": "^7.2.1",
        "immutable": "^4.0.0-rc.12",
        "menubar": "*",
        "moment": "^2.22.2",
        "react": "^16.6.1",
        "react-dom": "^16.6.1",
        "react-scripts-ts": "3.1.0",
        "shelljs": "^0.8.2"
    },
    "scripts": {
        "start": "react-scripts-ts start",
        "build": "react-scripts-ts build",
        "build-mac": "npm run clean; npm run build; cd ./build; npm install; npm run build-mac",
        "build-win": "npm run clean; npm run build; cd ./build; npm install; npm run build-win",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "watch": "npm-watch build",
        "clean": "rm -rf release-builds build"
    },
    "devDependencies": {
        "@types/jest": "^23.3.9",
        "@types/node": "^10.12.2",
        "@types/react": "^16.4.18",
        "@types/react-dom": "^16.0.9",
        "npm-watch": "^0.4.0",
        "typescript": "^3.1.6"
    },
    "watch": {
        "build": {
            "patterns": [
                "src",
                "package.json",
                "public"
            ],
            "ignore": "build",
            "extensions": "*",
            "quiet": false
        }
    }
}