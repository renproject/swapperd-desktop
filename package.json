{
  "name": "yarems",
  "version": "1.0.0",
  "description": "Yet Another React Electron Menubar Starter",
  "main": "main.ts",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "NODE_ENV=development electron .",
    "dev": "concurrently -k 'sleep 1; npm start' 'node -r @babel/register server.js'",
    "build": "NODE_ENV=production webpack --config webpack.config.production.babel.js",
    "package": "npm run build && electron-builder"
  },
  "author": "Emmanuel Pilande",
  "engine-strict": true,
  "engines": {
    "node": ">= 6.x",
    "npm": ">= 3.x"
  },
  "build": {
    "appId": "org.epilande.yarems",
    "productName": "Yarems",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "resources/",
      "index.html",
      "main.ts",
      "js/",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "resources/app.icns"
    }
  },
  "dependencies": {
    "@types/react-redux": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "bignumber.js": "^8.0.2",
    "fix-path": "^2.1.0",
    "immutable": "^4.0.0-rc.12",
    "menubar": "^5.2.3",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3",
    "typesafe-actions": "^3.0.0",
    "url-loader": "^1.1.2",
    "node-notifier": "^5.3.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "extract-zip": "^1.6.7",
    "request": "2.88.0",
    "shelljs": "^0.8.3",
    "sqlite3": "^4.0.6",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "devtron": "^1.4.0",
    "electron": "^4.0.3",
    "electron-builder": "^20.38.5",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "node-sass": "^4.11.0",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1"
  }
}
